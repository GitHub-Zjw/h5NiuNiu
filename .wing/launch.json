{
    "version": "0.2.0",
    "configurations": [{
            "name": "Wing 内置播放器调试",
            "type": "chrome",
            "request": "launch",
            "file": "index.html",
            //"url": "http://mysite.com/index.html",
            "runtimeExecutable": "${execPath}",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",
            "preLaunchTask": "build",
            "port": 5659
        },
        {
            "name": "使用本机 Chrome 调试",
            // "verboseDiagnosticLogging": true,
            "type": "chrome",
            "request": "launch",
            "file": "index.html",
            //"url": "http://mysite.com/index.html",
            "runtimeExecutable": "",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",
            "preLaunchTask": "build",
            "userDataDir": "${tmpdir}",
            "port": 5659
        },
        {
            "name": "使用 Chrome 调试",
            "type": "chrome",
            "request": "launch",
            "file": "index.html",
            "runtimeExecutable": "C:/Program Files (x86)/Google/Chrome/Application/chrome",
            "useBuildInServer": true,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}",
            "preLaunchTask":"build",
            "port":5659
        }
    ]
}